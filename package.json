    "napnux",
    "napnux cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "napnux": "*"
  },
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "14.x"
  }
}