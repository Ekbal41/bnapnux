{
  "name": "napnux-test",
  "version": "1.0.9",
  "description": "A project created with Napnux CLI: ./",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "nodemon": "nodemon --ignore node_modules --watch . index.js",
    "bsync": "browser-sync start --proxy localhost:3000 --watch --files='**/*' --no-ui --reload-delay 1000",
    "dev": "npm-run-all --parallel  nodemon bsync"
